                                 ; AssignmentAssembly.asm
                                 ;
                                 ; Created: 20/09/2019 11:32:22
                                 ; Author : Daniel
                                 ;
                                 ; Address is 0x27 but shifted 1 Left, with the lower bit being 0 to signify sending
                                 .equ SLA_W=0x4E
                                 
                                 .def zero_reg=r20
                                 .def sreg_temp=r11
                                 .def IS_HIGH=r18
                                 .def GOT_DISTANCE=r19
                                 .def temp=r21
                                 .def pulse_time_low=r16
                                 .def pulse_time_high=r17
                                 .def distance=r27
                                 .def div_result=r25
                                 .def twiDataToSend=r26
                                 .def tooSend=r22
                                 .def is_char=r23
                                 .def options=r24
                                 .def result_o=r28
                                 .def result_t=r29
                                 .def result_h=r30
                                 
                                 ; Replace with your application code
                                 .ORG 0x00
000000 c004                         rjmp start ; Reset interrupt
                                 .ORG 0x04
000004 c03b                         RJMP int1_int_start       ; Addr $04
                                 
                                 start:
                                 	;Initialize stack pointer
000005 e058                      	LDI temp, high(RAMEND)
000006 bf5e                      	OUT SPH, temp
000007 e55f                      	LDI temp, low(RAMEND)
000008 bf5d                      	OUT SPL, temp
                                 	
                                 	;Initalize GPIO registers for use
                                 	;TEMP to test ultrasonic 
000009 e051                      	LDI temp, 0x01
00000a bb57                      	OUT DDRB, temp
00000b 2744                      	CLR zero_reg
                                 	;
                                 
00000c ef57                      	LDI temp, 0xF7
00000d bb51                      	OUT DDRD, temp
00000e e050                      	LDI temp, 0x00
00000f bb58                      	OUT PORTB, temp
000010 bb55                      	OUT PORTC, temp
000011 c000                      	RJMP setup_lcd
                                 
                                 setup_lcd:
000012 940e 004f                 	CALL setSendRate
000014 940e 0059                 	CALL initLCD
000016 c000                      	RJMP setup_interrupts
                                 
                                 setup_interrupts:
                                 	;Enable INT1 interrupt
000017 e850                      	LDI temp, 0x80
000018 bf5b                      	OUT GICR, temp
                                 	;Set INT1 to trigger on any logical change
000019 e054                      	LDI temp, 0x04
00001a bf55                      	OUT MCUCR, temp
                                 	;Enable global interrupts
00001b 9478                      	SEI
00001c c000                      	RJMP main_loop
                                 
                                 main_loop:
00001d d004                      	RCALL query_sensors
00001e d0a2                      	RCALL generateSeperateNums
00001f d08a                      	RCALL updateLCD
000020 d0e9                      	RCALL wait_100_ms
                                 	;OUT PORTB, distance
                                 
000021 cffb                      	rjmp main_loop
                                 
                                 query_sensors:
                                 	;Start high pulse
000022 e051                      	LDI temp, 0x01
000023 bb58                      	OUT PORTB, temp
                                 	;10 us delay
000024 e258                      	LDI  temp, 0x28
000025 955a                      L7: DEC  temp
000026 f7f1                          BRNE L7
                                 	;Continue by setting output to low
000027 e050                      	LDI temp, 0x00
000028 bb58                      	OUT PORTB, temp
000029 e030                      	LDI GOT_DISTANCE, 0x00
                                 	;Save a clock cycle by reusing the fact that got_distance is 0x00
                                 	;OUT PORTC, GOT_DISTANCE 
                                 ;Wait until ultrasonic sensor has finished its echo pulse
                                 wait_until_done:
00002a 3030                      	CPI GOT_DISTANCE, 0x00
00002b f3f1                      	BREQ wait_until_done
                                 	;LDI pulse_time_low, 0x80
                                 	;LDI pulse_time_high, 0x03
                                 
                                 	;Make sure old records are gone
                                 	;CLR pulse_time_low
                                 	;CLR pulse_time_high
                                 	;Record the distance into register
00002c b75f                      	IN temp, SREG
00002d 94f8                      	CLI
00002e b50c                      	IN pulse_time_low, TCNT1L
00002f b51d                      	IN pulse_time_high, TCNT1H	
                                 
000030 bd4d                      	OUT TCNT1H, zero_reg
000031 bd4c                      	OUT TCNT1L, zero_reg
000032 bf5f                      	OUT SREG, temp
                                 	;Reset the clock
                                 	;OUT PORTB, pulse_time_low
                                 
000033 2799                      	CLR div_result
000034 27bb                      	CLR distance
                                 
                                 
                                 check_div_done:
                                 	;Check if less than 57
000035 3507                      	CPI pulse_time_low, 0x57
000036 f008                      	BRLO check_high
                                 	;Not done yet
000037 c002                      	RJMP calculate_distance
                                 
                                 check_high:
000038 3010                      	CPI pulse_time_high, 0x00
                                 	;go to display
000039 f029                      	BREQ display_distance
                                 
                                 calculate_distance:
00003a 5507                      	SUBI pulse_time_low, 0x57
00003b 4010                      	SBCI pulse_time_high, 0x00
00003c 9488                      	CLC
00003d 95b3                      	INC distance
00003e cff6                      	RJMP check_div_done
                                 
                                 display_distance:
00003f 9508                      	RET
                                 
                                 ;Int1 routine
                                 int1_int_start:
000040 b6bf                      	IN sreg_temp,SREG
000041 935f                      	PUSH temp
000042 3021                      	CPI IS_HIGH,0x01
000043 f421                      	BRNE INT1_NOT_HIGH
                                 
                                 INT1_HIGH:
                                 	;Turn off counter/timer
000044 bd4e                      	OUT TCCR1B,zero_reg
                                 
                                 	;Got the distance now
000045 e031                      	LDI GOT_DISTANCE, 0x01
                                 	;Pulse now low
000046 e020                      	LDI IS_HIGH,0x00
000047 c004                      	RJMP END_INT1
                                 
                                 INT1_NOT_HIGH:
000048 b55e                      	IN temp, TCCR1B
                                 	;Load timer with prescale factor of 8
000049 6052                      	ORI temp,0x02 
                                 	;Start timer
00004a bd5e                      	OUT TCCR1B,temp
                                 	;Echo pulse now high
00004b e021                      	LDI IS_HIGH,0x01
                                 
                                 END_INT1:
00004c 915f                      	POP temp
00004d bebf                      	OUT SREG,sreg_temp
00004e 9518                      	RETI
                                 
                                 setSendRate:
00004f 2755                      	CLR temp
000050 b951                      	OUT TWSR, temp
000051 e750                      	LDI temp, 0x70
000052 b950                      	OUT TWBR, temp
000053 e054                      	LDI temp, (1 << TWEN)
000054 bf56                      	OUT TWCR, temp
000055 ef5f                      	LDI temp, 0xFF
000056 b953                      	OUT TWDR, temp
000057 9508                      	RET
                                 
                                 sendTWI:
000058 c012                      	RJMP twiStart
                                 
                                 initLCD:
                                 	;Might need to add delay
000059 2777                      	CLR is_char
00005a e060                      	LDI tooSend, 0x00
00005b d035                      	RCALL lcdSend
00005c d078                      	RCALL wait_5_ms
